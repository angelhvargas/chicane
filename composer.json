{
    "name": "angelvargas/chicane",
    "description": "Lightweight PHP framework",
    "type": "library",
    "require": {
        "doctrine/orm": "^2.6",
        "filp/whoops": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/cache": "^4.0",
        "symfony/config": "^4.0",
        "symfony/debug-pack": "^1.0",
        "symfony/event-dispatcher": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/maker-bundle": "^1.3",
        "symfony/routing": "^4.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "classmap": [
            "core"
        ],
        "psr-4": {
            "Chicane\\App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "license": "GNU GENERAL PUBLIC LICENSE",
    "authors": [
        {
            "name": "Angel Vargas",
            "email": "angelvargas@outlook.es"
        }
    ]
}
